{
  "extends": ["daangemist"],
  "env": {
    "browser": true,
    "node": true,
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json",
  },
  "rules": {
    "no-lonely-if": "error",
    "no-console": "error",
    "@typescript-eslint/consistent-type-imports": "error",
  },
}
